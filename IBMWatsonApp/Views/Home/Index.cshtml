@{
    ViewData["Title"] = "Home Page";
}
<h1>Welcome to IBM Watson</h1>
<h2>Use Case Details:</h2>
<div class="row">
    <div class="col-md-6">
        <h3>Book a Lab test</h3>
        <ul>
            <li>User wants to book a lab test. </li>
            <li>Provide test details including procedure, cost.</li>
            <li>Book appointment for lab test with Date and Time</li>
            <li>Send confirmation email.</li>
        </ul>
    </div>
    <div class="col-md-6">
        <h3>Symptom driven doctors</h3>
        <ul>
            <li>Ask for symptoms</li>
            <li>Ask for location</li>
            <li>Suggest doctor in that location </li>
            <li>Book appointment with Doctor with Date and Time</li>
            <li>Send confirmation email.</li>
        </ul>
    </div>
</div>
